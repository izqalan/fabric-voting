    "host": "localhost:8081",
    "basePath": "/api/v1",
    "paths": {
        "/hello": {
            "get": {
                "description": "Returns hello world",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Signal"
                ],
                "summary": "Hello World",
                "responses": {
                    "200": {
                        "description": "hello world",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/ping": {
            "get": {
                "description": "Returns pong",
                "produces": [
                    "text/plain"
                ],
                "tags": [
                    "Signal"
                ],
                "summary": "Ping Pong",
                "responses": {
                    "200": {
                        "description": "pong",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}